{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "3d-web-db",
      "collectionName": "Billers",
      "createIfNotExists": true,
      "connectionStringSetting": "3dwebdb_DOCUMENTDB"
    }
  ],
  "scriptFile": "../dist/edit-customer/index.js"
}
